{
  "system": {
     "modules": [
       {"path":"./subfolder/localmodules", "names": "{all}" },
       {"path":"{app_dir}/app", "names": "{all}" },
       {"path":"{massir_dir}/modules", "names": ["system_logger"] }
     ]
   },
  "template": {
    "project_banner_template": "\n===========================================\n\t{project_name}\n\t{project_version}\n\t{project_info}\n",
    "system_log_template": "[{level}]\t{message}",
    "banner_color_code": "32",
    "system_log_color_code": "96"
  },
  "logs": {
    "show_logs": true,
    "show_banner": true,
    "hide_log_tags": ["", ""],
    "hide_log_levels": [],
    "debug_mode": true
  },
  "information": {
    "project_name": "Massir Project",
    "project_version": "0.0.4 Alpha - on app_settings"
  },

  "fastapi_provider": {
    "web": {
      "host": "127.0.0.1",
      "port": 8000,
      "reload": false
    }
  }
}
